<p>[exception] {{ status_code ~ ' | ' ~ status_text ~ ' | ' ~ exception.class }}</p>
<p>[message] {{ exception.message }}</p>

{% if request is not same as(null) %}
<p>
    [request] {{ request.pathInfo }}<br>
    [user] {{ request.user }}
</p>
{% endif %}

<pre>
{% for i, e in exception.toarray %}
    [{{ i + 1 }}] {{ e.class }}: {{ e.message }}
    {% include '@Twig/Exception/traces.txt.twig' with { 'exception': e } only %}
{% endfor %}
</pre>
